{
  "type": "module",
  "name": "uxiu-template-default",
  "version": "1.3.0",
  "description": "uxiu 默认模板",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "start:dist": "node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
    "debug": "cross-env NODE_ENV=development tsx watch --inspect ./src/index.ts",
    "build": "uxiu-cli build -p=./public"
  },
  "keywords": [
    "uxiu-template-default",
    "uxiu-cli",
    "uxiu"
  ],
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fy-store/uxiu-template-default.git"
  },
  "author": "feiYu <feiyucode@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@koa/bodyparser": "^5.1.1",
    "@koa/cors": "^5.0.0",
    "koa": "^2.15.4",
    "koa-router": "^13.0.1",
    "koa-static": "^5.0.0",
    "uxiu": "0.6.1"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/node": "^22.10.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "uxiu-cli": "^0.5.0"
  }
}